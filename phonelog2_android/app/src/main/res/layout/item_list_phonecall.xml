<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layout0"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="5dp"
    android:onClick="perform_action_click_select_contact">

    <TextView
        android:id="@+id/labelDay0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="50dp"
        android:text="@+id/labelDay"
        android:textSize="12sp" />
    <TextView
        android:id="@+id/labelNumber0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/labelDay0"
        android:layout_toRightOf="@+id/logoPhoto0"
        android:text="@+id/labelNumber"
        android:textColor="#CCC"
        android:textSize="12sp" />
    <ImageView
        android:id="@+id/logoPhoto0"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignTop="@+id/labelNumber0"
        android:layout_marginRight="6dip"
        android:src="@drawable/default_contact_logo"
        android:contentDescription="TODO" />

    <TextView
        android:id="@+id/logoPhotoText0"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignTop="@+id/labelNumber0"
        android:layout_marginRight="6dip"
        android:background="#ff0000"
        android:gravity="center"
        android:text="B"
        android:textSize="40dp"
        android:visibility="invisible"
        android:clickable="true"
        />





    <TextView
        android:id="@+id/labelName0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/labelNumber0"
        android:layout_toRightOf="@+id/logoPhoto0"
        android:text="@+id/labelNumber"
        android:textColor="#CCC"
        android:textSize="12sp" />

    <com.beardedhen.androidbootstrap.AwesomeTextView
        android:id="@+id/logoType0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/labelNumber0"
        android:layout_marginRight="3dp"
        android:layout_toLeftOf="@+id/logoPhoneOuMessage0"
        android:textSize="12sp" />

    <com.beardedhen.androidbootstrap.AwesomeTextView
        android:id="@+id/logoPhoneOuMessage0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/labelNumber0"
        android:layout_marginRight="3dp"
        android:layout_toLeftOf="@+id/labelDDate0"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/labelDDate0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/labelNumber0"
        android:layout_alignParentRight="true"
        android:text=""
        android:textColor="#000"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/labelComment0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/labelName0"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@+id/logoPhoto0"
        android:text="@+id/labelComment0"
        android:textSize="16sp" />



</RelativeLayout>