    <ImageView
        android:id="@+id/logo_androidPhoneSystem"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@+id/buttonPrivateList"
        android:contentDescription="@string/TitreAndroidPhoneSystem"
        android:src="@drawable/logo_dialer" />

    <Button
        android:id="@+id/buttonAndroidPhoneSystem"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@+id/buttonPrivateList"
        android:background="#ccffffff"
